#!/bin/bash

data_dir=$1
newfilename=$2
cp ../$1/n2run10/nparticle2_nsim_rho0.1T0.04_train_sampled.pt ../$1/tmp.pt
tmp=`ls ../$data_dir/n2/tmp.pt`

for i in {0..2}; do

    filename=`ls ../$1/n2run10/*_id${i}.pt`
    echo $filename
    python combine2files.py $tmp $filename $tmp

done
echo $tmp
echo $combinefilename
mv $tmp $combinefilename
